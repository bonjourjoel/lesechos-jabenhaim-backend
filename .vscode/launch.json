  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "start:dev",
      "program": "${workspaceFolder}/node_modules/@nestjs/cli/bin/nest.js",
      "args": ["start", "--watch"],
      "envFile": "${workspaceFolder}/.env.local",
      "env": {
        "NODE_ENV": "local"
      },
      "runtimeArgs": ["--trace-warnings"],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    }
  ]
}
